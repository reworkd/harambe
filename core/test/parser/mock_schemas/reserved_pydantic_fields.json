{
  "$schema": "https://raw.githubusercontent.com/reworkd/harambe/refs/heads/main/schema.json",
  "model_config": {
    "type": "array",
    "items": {
      "type": "integer"
    }
  },
  "model_type": {
    "type": "string"
  },
  "model_name": {
    "type": "string"
  },
  "model_id": {
    "type": "integer"
  },
  "model_dump": {
    "type": "string"
  },
  "model_extra": {
    "type": "string",
    "expression": "CONCAT_WS('_', model_name, model_type)"
  },
  "model_json": {
    "type": "object",
    "properties": {
      "model_validate": {
        "type": "string"
      },
      "model_dump": {
        "type": "integer"
      },
      "model_config": {
        "type": "array",
        "items": {
          "type": "integer"
        }
      }
    }
  }

}